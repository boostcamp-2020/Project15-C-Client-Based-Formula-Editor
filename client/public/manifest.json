{
  "name": "Boost Restaurant",
  "version": "1.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["/main.js"]
    }
  ],
  "web_accessible_resources": ["/content.css"]
}
